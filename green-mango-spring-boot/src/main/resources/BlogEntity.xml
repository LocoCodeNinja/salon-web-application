<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.BlogEntity">
        <table name="blog" schema="greenmango" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="title">
                <column name="title"/>
            </basic>
            <basic name="author">
                <column name="author"/>
            </basic>
            <basic name="content">
                <column name="content"/>
            </basic>
            <basic name="publicationDate">
                <column name="publication_date" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
