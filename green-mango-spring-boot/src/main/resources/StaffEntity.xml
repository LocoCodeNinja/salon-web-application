<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.StaffEntity">
        <table name="staff" schema="greenmango" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="title">
                <column name="title"/>
            </basic>
            <basic name="specialty">
                <column name="specialty"/>
            </basic>
            <basic name="photo">
                <column name="photo"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
