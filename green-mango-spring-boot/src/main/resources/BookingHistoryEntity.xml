<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.BookingHistoryEntity">
        <table name="booking_history" schema="greenmango" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="appointmentId">
                <column name="appointment_id" precision="-1"/>
            </basic>
            <basic name="note">
                <column name="note"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
